{
  "name": "copabr-dynamic-info",
  "version": "1.0.0",
  "description": "A simple program to fetch runner information from the internet",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "fetch": "node index.js",
    "build": "pkg . --targets node18-win-x64,node18-linux-x64 --output-path dist",
    "build:win": "pkg . --targets node18-win-x64 --output-path dist",
    "build:linux": "pkg . --targets node18-linux-x64 --output-path dist"
  },
  "keywords": [
    "runner",
    "fetch",
    "information"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "pkg": {
    "assets": [
      "config.json"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
